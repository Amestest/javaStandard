package javaStandard;

public class OperatorEx11 {

	public static void main(String[] args) {
		char a = 'a';
		char b = 'b';
		char zero = '0';
		char two = '2';

		System.out.printf("'%c' - '%c' = %d%n", d, a, d - a); // 'd'-'a' = 3 
		System.out.printf("'%c' - '%c' = %d%n" two, zero, two -zero);
		System.our.printf()
	}

}
