package javaStandard;

class Car3 {
	String color; // 색상
	String gearType; // 변속기 종류 - auto(자동), manual(수동)
	int door; //  문의 개수

	Car3() {
		this("white","auto",4);
	}
	
	Car3(Car3 c) {
		color = c.color;
		gearType = c.gearType;
		door = c.door;
		}
	Car3(String color, String gearType, int door) {
		this.color = color;
		this.gearType = gearType;
		this.door = door;
	}
	}
class CarTest3 {
	public static void main(String[] args) {
		
	}
}
